{
  "manifest_version": 2,

  "name": "RTP Download",
  "description": "Get the link for download video in RTP web page",
  "version": "1.0",
   "author": "Antonio Oliveira",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Obter link" 
  },
  "permissions": [
    "activeTab",
    "https://rtp-downloader.herokuapp.com/*" 
  ]
}